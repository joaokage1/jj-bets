<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg xmlns="http://www.w3.org/2000/svg" width="300" height="200" viewBox="0 0 300 200">
  <!-- Fundo escuro -->
  <rect width="300" height="200" fill="#2d6a4f" />
  
  <!-- Padrão de fundo -->
  <rect width="300" height="200" fill="#2d6a4f" rx="10" ry="10" />
  <path d="M0,0 L300,200 M300,0 L0,200" stroke="#1e5c41" stroke-width="2" stroke-opacity="0.3" />
  
  <!-- Contorno de interrogação grande -->
  <text x="150" y="120" font-family="Arial" font-size="120" fill="none" stroke="#ffffff" stroke-width="3" 
        stroke-opacity="0.3" text-anchor="middle" font-weight="bold">?</text>
  
  <!-- Cartas de baralho com pontos de interrogação -->
  <!-- Carta 1 -->
  <g transform="translate(70, 60) rotate(-15)">
    <rect width="50" height="70" rx="5" ry="5" fill="#f5f5f5" stroke="#000000" stroke-width="1" />
    <text x="25" y="45" font-family="Arial" font-size="30" font-weight="bold" fill="#6d6875" text-anchor="middle">?</text>
  </g>
  
  <!-- Carta 2 -->
  <g transform="translate(150, 50) rotate(5)">
    <rect width="50" height="70" rx="5" ry="5" fill="#f5f5f5" stroke="#000000" stroke-width="1" />
    <text x="25" y="45" font-family="Arial" font-size="30" font-weight="bold" fill="#6d6875" text-anchor="middle">?</text>
  </g>
  
  <!-- Carta 3 -->
  <g transform="translate(210, 70) rotate(20)">
    <rect width="50" height="70" rx="5" ry="5" fill="#f5f5f5" stroke="#000000" stroke-width="1" />
    <text x="25" y="45" font-family="Arial" font-size="30" font-weight="bold" fill="#6d6875" text-anchor="middle">?</text>
  </g>
  
  <!-- Dado -->
  <g transform="translate(120, 140) rotate(-10)">
    <rect width="30" height="30" rx="5" ry="5" fill="#ffffff" stroke="#000000" stroke-width="1" />
    <circle cx="10" cy="10" r="3" fill="#000000" />
    <circle cx="20" cy="20" r="3" fill="#000000" />
    <circle cx="10" cy="20" r="3" fill="#000000" />
    <circle cx="20" cy="10" r="3" fill="#000000" />
  </g>
  
  <!-- Dado 2 -->
  <g transform="translate(180, 150) rotate(15)">
    <rect width="30" height="30" rx="5" ry="5" fill="#ffffff" stroke="#000000" stroke-width="1" />
    <circle cx="15" cy="15" r="3" fill="#000000" />
  </g>
  
  <!-- Texto principal -->
  <text x="150" y="40" font-family="Arial" font-size="24" fill="#000000" text-anchor="middle" 
        font-weight="bold" opacity="0.5">EM BREVE</text>
  <text x="150" y="38" font-family="Arial" font-size="24" fill="#ffd166" text-anchor="middle" font-weight="bold">EM BREVE</text>
  
  <!-- Texto secundário -->
  <text x="150" y="180" font-family="Arial" font-size="14" fill="#ffffff" text-anchor="middle" opacity="0.8">Novos jogos chegando!</text>
  
  <!-- Brilhos decorativos -->
  <circle cx="50" cy="30" r="3" fill="white" opacity="0.5" />
  <circle cx="250" cy="40" r="4" fill="white" opacity="0.4" />
  <circle cx="270" cy="170" r="2" fill="white" opacity="0.6" />
  <circle cx="30" cy="160" r="2" fill="white" opacity="0.5" />
</svg>
